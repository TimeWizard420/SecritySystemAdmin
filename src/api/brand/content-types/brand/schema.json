{
  "kind": "collectionType",
  "collectionName": "brands",
  "info": {
    "singularName": "brand",
    "pluralName": "brands",
    "displayName": "brands",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "richtext",
      "minLength": 50
    },
    "website": {
      "type": "string",
      "unique": false
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "brand"
    },
    "review": {
      "type": "component",
      "repeatable": true,
      "component": "brand-componenets.brand-reviews"
    }
  }
}
